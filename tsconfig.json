{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2021",
      "DOM"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "types": [
      "nativewind/types"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "app/components/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    ".expo"
  ],
  "extends": "expo/tsconfig.base"
}
